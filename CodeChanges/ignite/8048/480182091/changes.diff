-                            }
-                        }
-                    }
-                    else if (transacted) {
-                        try {
-                            session.commit();
-                        }
-                        catch (JMSException e) {
-                            log.warning("Could not commit JMS session (non-batched).", e);
-                        }
-                    }
-                }
-            });
-
-        }
-
-        public void resetBatchCounter() {
-            counter.set(0);
-        }
-    }
