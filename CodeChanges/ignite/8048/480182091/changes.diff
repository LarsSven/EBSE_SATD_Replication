-    private void processMessage(T message) {
-        final IgniteDataStreamer<K, V> streamer = getStreamer();
-
-        Map<K, V> entries = transformer.apply(message);
-
-        if (entries == null || entries.size() == 0)
-            return;
-
-        streamer.addData(entries);
-    }
