-            executor.execute(new Runnable() {
-                @Override @SuppressWarnings("unchecked")
-                public void run() {
-                    processMessage((T)message);
-                    if (batched) {
-                        // batch completion may be handled by timer only
-                        if (batchClosureSize <= 0)
-                            return;
