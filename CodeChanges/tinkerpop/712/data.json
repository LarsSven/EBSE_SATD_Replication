    "starred_url": "https://api.github.com/users/FlorianHockmann/starred{/owner}{/repo}",
    "subscriptions_url": "https://api.github.com/users/FlorianHockmann/subscriptions",
    "organizations_url": "https://api.github.com/users/FlorianHockmann/orgs",
    "repos_url": "https://api.github.com/users/FlorianHockmann/repos",
    "events_url": "https://api.github.com/users/FlorianHockmann/events{/privacy}",
