  "title": "CLOUDSTACK-9888: Duplicate usage while processing usage events ",
  "user": {
    "login": "jayakarteek",
    "id": 23551298,
    "node_id": "MDQ6VXNlcjIzNTUxMjk4",
    "avatar_url": "https://avatars.githubusercontent.com/u/23551298?v=4",
    "gravatar_id": "",
    "url": "https://api.github.com/users/jayakarteek",
    "html_url": "https://github.com/jayakarteek",
    "followers_url": "https://api.github.com/users/jayakarteek/followers",
    "following_url": "https://api.github.com/users/jayakarteek/following{/other_user}",
    "gists_url": "https://api.github.com/users/jayakarteek/gists{/gist_id}",
    "starred_url": "https://api.github.com/users/jayakarteek/starred{/owner}{/repo}",
    "subscriptions_url": "https://api.github.com/users/jayakarteek/subscriptions",
    "organizations_url": "https://api.github.com/users/jayakarteek/orgs",
