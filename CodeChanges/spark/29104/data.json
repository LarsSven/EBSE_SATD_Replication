    "followers_url": "https://api.github.com/users/leanken-zz/followers",
    "following_url": "https://api.github.com/users/leanken-zz/following{/other_user}",
    "gists_url": "https://api.github.com/users/leanken-zz/gists{/gist_id}",
    "starred_url": "https://api.github.com/users/leanken-zz/starred{/owner}{/repo}",
    "subscriptions_url": "https://api.github.com/users/leanken-zz/subscriptions",
    "organizations_url": "https://api.github.com/users/leanken-zz/orgs",
    "repos_url": "https://api.github.com/users/leanken-zz/repos",
    "events_url": "https://api.github.com/users/leanken-zz/events{/privacy}",
    "received_events_url": "https://api.github.com/users/leanken-zz/received_events",
