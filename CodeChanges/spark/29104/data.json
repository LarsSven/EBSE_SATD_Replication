  "title": "[SPARK-32290][SQL] SingleColumn Null Aware Anti Join Optimize",
  "user": {
    "login": "leanken-zz",
    "id": 17242071,
    "node_id": "MDQ6VXNlcjE3MjQyMDcx",
    "avatar_url": "https://avatars.githubusercontent.com/u/17242071?v=4",
    "gravatar_id": "",
    "url": "https://api.github.com/users/leanken-zz",
