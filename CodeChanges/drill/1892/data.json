    "html_url": "https://github.com/cgivre",
    "followers_url": "https://api.github.com/users/cgivre/followers",
    "following_url": "https://api.github.com/users/cgivre/following{/other_user}",
    "gists_url": "https://api.github.com/users/cgivre/gists{/gist_id}",
    "starred_url": "https://api.github.com/users/cgivre/starred{/owner}{/repo}",
    "subscriptions_url": "https://api.github.com/users/cgivre/subscriptions",
    "organizations_url": "https://api.github.com/users/cgivre/orgs",
    "repos_url": "https://api.github.com/users/cgivre/repos",
    "events_url": "https://api.github.com/users/cgivre/events{/privacy}",
