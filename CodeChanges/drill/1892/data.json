  "title": "DRILL-7437: Storage Plugin for Generic HTTP REST API",
  "user": {
    "login": "cgivre",
    "id": 5513150,
    "node_id": "MDQ6VXNlcjU1MTMxNTA=",
    "avatar_url": "https://avatars.githubusercontent.com/u/5513150?v=4",
    "gravatar_id": "",
